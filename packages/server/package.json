{
  "name": "@agent-e/server",
  "version": "1.8.0",
  "description": "Ready-to-run HTTP + WebSocket server for AgentE",
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GameDesignerMohamed/Agent-E.git",
    "directory": "packages/server"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "agent-e-server": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "economy",
    "balancer",
    "game-economy",
    "server",
    "websocket",
    "autonomous"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@agent-e/core": "^1.8.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "*",
    "typescript": "*"
  }
}
